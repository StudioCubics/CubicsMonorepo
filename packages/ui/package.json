{
  "name": "@studiocubics/ui",
  "author": {
    "name": "Studio Cubics",
    "email": "studiocubics7@gmail.com",
    "url": "https://studio-cubics.vercel.app"
  },
  "version": "0.0.3",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "svgr -- src/icons/ && rollup --config",
    "dev": "svgr -- src/icons/ && rollup --config --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rmdir /s /q .turbo; rmdir /s /q dist"
  },
  "peerDependencies": {
    "@react-email/components": "^0.0.25",
    "react": "^18.2.0",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@studiocubics/eslint-config": "*",
    "@studiocubics/typescript-config": "*",
    "@svgr/cli": "^8.1.0",
    "@svgr/rollup": "^8.1.0",
    "@types/react": "^18.2.61",
    "rollup": "^4.24.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-banner2": "^1.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.79.5",
    "typescript": "^5.3.3"
  }
}
